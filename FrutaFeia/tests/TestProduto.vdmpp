class TestProduto is subclass of TestFrutaFeia
instance variables
	produto: Produto := new Produto("Maca", "Golden", 5);

operations
  public testConstructor() == (
    IO`println("\t\t Testing Constructor");
    assertEqual("Maca", produto.nome);
    assertEqual("Golden", produto.origem);
    assertEqual(5, produto.peso);
  );
  
  public testAdicionaAndRemovePeso() == (
		IO`println("\t\t AdicionaAndRemovePeso");
  );

  public testAll() == (
    IO`println("\t Testing Produtor class");
    testConstructor();
    testAdicionaAndRemovePeso();
  );

end TestProduto