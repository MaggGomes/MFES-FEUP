class TestEncomenda is subclass of TestFrutaFeia
instance variables
	agricultor: Agricultor := new Agricultor("Pedro", "Mirandela");
	produto: Produto := new Produto("Maca", "Golden", 5);
	encomenda: Encomenda := new Encomenda();

operations
  public testConstructor() == (
    IO`println("\t\t Testing Constructor");
    assertEqual({} , encomenda.vende);
  );
  
  public testAdicionaAndRemoveProduto() == (
  	IO`println("\t\t Testing AdicionaProdutor");
  	assertEqual(0, card encomenda.produto); 
  	encomenda.adicionaProduto(produto);
  	assertEqual(1, card encomenda.produto);  
  	encomenda.adicionaProduto(produto);
  	assertEqual(0, card encomenda.produto);  
  );

  public testAll() == (
    IO`println("\t Testing Encomenda class");
    testConstructor();
    testAdicionaAndRemoveProduto()
  );

end TestEncomenda